{
  "name": "angular-input-focus-tester",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "auto-test": "ng test angular-input-focus --browsers ChromeHeadless --reporters junit --code-coverage --watch=false",
    "ci-build": "ng build angular-input-focus",
    "coverage": "codecov",
    "build-lib": "ng build angular-input-focus --prod && npm run copy-license && npm run copy-readme",
    "patch-version": "cd projects/angular-input-focus && npm version patch && cd ../../",
    "copy-license": "cp ./LICENSE ./dist/angular-input-focus",
    "copy-readme": "cp ./README.md ./dist/angular-input-focus",
    "publish-lib": "npm run patch-version && npm run build-lib && npm publish ./dist/angular-input-focus"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^13.3.6",
    "@angular/common": "^13.3.6",
    "@angular/compiler": "^13.3.6",
    "@angular/core": "^13.3.6",
    "@angular/forms": "^13.3.6",
    "@angular/platform-browser": "^13.3.6",
    "@angular/platform-browser-dynamic": "^13.3.6",
    "@angular/router": "^13.3.6",
    "rxjs": "~6.6.7",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.3.5",
    "@angular/cli": "^13.3.5",
    "@angular/compiler-cli": "^13.3.6",
    "@angular/language-service": "^13.3.6",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "codecov": "^3.8.3",
    "codelyzer": "^6.0.0",
    "jasmine-core": "~3.8",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~6.3.19",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "^2.2.0",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "karma-junit-reporter": "^1.2.0",
    "ng-packagr": "^13.3.1",
    "protractor": "~7.0.0",
    "ts-node": "~7.0.0",
    "tslint": "~6.1.0",
    "typescript": "~4.6.4"
  }
}
